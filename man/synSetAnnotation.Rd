\name{synSetAnnotation}
\alias{synSetAnnotation}
\docType{methods}
\title{
synSetAnnotation
}
\description{
Set an annotation on an entity.
}
\usage{synSetAnnotation(entity, name)<-value}
\arguments{
	\item{entity}{
	The entity on which to set the annotation.
	}
	\item{name}{
	The name of the annotation to set.
	}
	\item{value}{
	The value of the annotation to set.
	}
}
