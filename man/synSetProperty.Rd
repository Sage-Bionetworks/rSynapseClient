\name{synSetProperty}
\alias{synSetProperty}
\docType{methods}
\title{
synSetProperty
}
\description{
Set a property on an entity.
}
\usage{synSetProperty(entity, name)<-value}
\arguments{
	\item{entity}{
	The entity on which to set the property.
	}
	\item{name}{
	The name of the property to set.
	}
	\item{value}{
	The value of the property to set.
	}
}
